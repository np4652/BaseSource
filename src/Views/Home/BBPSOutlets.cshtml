@model Response<List<BBPSOutlet>>
@if (Model.StatusCode == 1)
{
    if (Model.Data != null && Model.Data.Any())
    {
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th></th>
                    <th>Agent Name</th>
                    <th>Shop Name</th>
                    <th>Mobile</th>
                    <th>Address</th>
                    <th>Pincode</th>
                    <th>City</th>
                    <th>State</th>
                </tr>
            </thead>
            <tbody>
                @{
                    int i = 0;
                    foreach (var item in Model.Data)
                    {
                        i++;
                        <tr>
                            <td>@i</td>
                            <td>@item.AgentName</td>
                            <td>@item.ShopName</td>
                            <td>@item.Mobile</td>
                            <td>@item.Address</td>
                            <td>@item.PinCode</td>
                            <td>@item.City</td>
                            <td>@item.State</td>
                        </tr>
                    }}
            </tbody>
        </table>
    }
}